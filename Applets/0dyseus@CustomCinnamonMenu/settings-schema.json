		"type": "entry",
		"dependency": "pref_remember_recently_used_apps",
		"default": "Recent Apps",
		"description": "Custom label for category",
		"tooltip": "Enter custom label for the Recently used apps category."
	},
	"pref_max_recently_used_apps": {
		"type": "spinbutton",
		"dependency": "pref_remember_recently_used_apps",
		"default": 20,
		"min": 5,
