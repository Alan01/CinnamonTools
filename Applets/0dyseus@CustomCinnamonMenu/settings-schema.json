		"type": "spinbutton",
		"dependency": "pref_show_custom_commands_box",
		"default": 0,
		"min": 0,
		"max": 2048,
		"step": 1,
		"indent": true,
		"units": "pixels",
		"description": "Custom width for search box:",
		"tooltip": "Set to 0 (zero) for not setting a width."
	},
	"sep1": {
		"type": "separator"
