#: applet.js:731
msgid "Configure the window list"
msgstr "Fensterliste konfigurieren …"

#: applet.js:743
msgid "Move to the other monitor"
msgstr "Zu anderem Bildschirm verschieben"

#: applet.js:752
#, javascript-format
msgid "Move to monitor %d"
msgstr "Auf Bildschirm %d verschieben"

#: applet.js:763
msgid "Only on this workspace"
msgstr "Nur auf dieser Arbeitsfläche"

#: applet.js:769
msgid "Visible on all workspaces"
msgstr "Auf allen Arbeitsflächen anzeigen"

#: applet.js:775
msgid "Move to another workspace"
msgstr "Auf andere Arbeitsfläche verschieben"

#. Close all/others
#: applet.js:797
msgid "Close all"
msgstr "Alle schließen"

#: applet.js:806
msgid "Close others"
msgstr "Andere schließen"

#: applet.js:820
msgid "Restore to full opacity"
msgstr "Die volle Deckkraft wiederherstellen"

#: applet.js:828
msgid "Restore"
msgstr "Wiederherstellen"

#: applet.js:833
msgid "Minimize"
msgstr "Verkleinern"

#: applet.js:841
msgid "Unmaximize"
msgstr "Wiederherstellen"

#: applet.js:846
msgid "Maximize"
msgstr "Vergrößern"

#: applet.js:853
msgid "Close"
msgstr "Schließen"

#. 0dyseus@window-list-fork->metadata.json->description
msgid "Fork of the main Cinnamon Window list applet."
msgstr "Fork des Cinnamon-Haupfensterlisten-Applets"

#. 0dyseus@window-list-fork->metadata.json->name
msgid "Window list (Fork By Odyseus)"
msgstr "Fensterliste (Fork von Odyseus)"

#. 0dyseus@window-list-fork->settings-
#. schema.json->pref_hide_labels->description
msgid "Hide labels in the window list buttons. (Cinnamon will be restarted)"
msgstr "Beschriftungen auf den Knöpfen der Fensterliste verbergen. (Cinnamon wird neu gestartet)"

#. 0dyseus@window-list-fork->settings-
#. schema.json->pref_reverse_scrolling->description
msgid "Reverse the direction of mouse-wheel scrolling in the window list"
msgstr "Die Richtung des Mausradrollens in der Fensterliste umkehren"

#. 0dyseus@window-list-fork->settings-
#. schema.json->pref_enable_scrolling->description
msgid "Enable mouse-wheel scrolling in the window list"
msgstr "Mausradrollen in der Fensterliste aktivieren"

#. 0dyseus@window-list-fork->settings-
#. schema.json->pref_window_preview->description
msgid "Show window thumbnails on hover"
msgstr "Fenstervorschau beim Darüberbewegen anzeigen"

#. 0dyseus@window-list-fork->settings-
#. schema.json->pref_buttons_use_entire_space->description
msgid ""
"Window buttons can have different sizes and use the entire space available"
msgstr ""
"Die Fensterknöpfe können verschiedene Größen haben und den verfügbaren Platz ausfüllen"

#. 0dyseus@window-list-fork->settings-
#. schema.json->pref_middle_click_close->description
msgid "Middle click to close window"
msgstr "Mittelklick, um das Fenster zu schließen"

#. 0dyseus@window-list-fork->settings-
#. schema.json->pref_hide_tooltips->description
msgid "Do not display tooltips. (Cinnamon will be restarted)"
msgstr "Tooltips nicht anzeigen. (Cinnamon wird neu gestartet)"

#. 0dyseus@window-list-fork->settings-
#. schema.json->pref_enable_alerts->description
msgid ""
"Show an alert in the window list when a window from another workspace "
"requires attention"
msgstr "Einen Hinweis in der Fensterliste anzeigen, wenn ein Fenster auf einer anderen Arbeitsfläche Aufmerksamkeit verlangt"
