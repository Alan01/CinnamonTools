#. 3.2->settings-schema.json->pref_hide_labels->description
#. 0dyseus@window-list-fork->settings-
#. schema.json->pref_hide_labels->description
msgid "Hide labels in the window list buttons"
msgstr "Ocultar etiquetas de los botones de la lista de ventanas"

#. 3.2->settings-schema.json->pref_invert_menu_items_order->description
#. 0dyseus@window-list-fork->settings-
#. schema.json->pref_invert_menu_items_order->description
msgid "Invert context menu items order"
msgstr "Invertir el orden de los ítems en el menú contextual"

#. 3.2->settings-schema.json->pref_reverse_scrolling->description
#. 0dyseus@window-list-fork->settings-
#. schema.json->pref_reverse_scrolling->description
msgid "Reverse the direction of mouse-wheel scrolling in the window list"
msgstr "Invertir la dirección de desplazamiento del ratón en la lista de ventanas"

#. 3.2->settings-schema.json->pref_enable_scrolling->description
#. 0dyseus@window-list-fork->settings-
#. schema.json->pref_enable_scrolling->description
msgid "Enable mouse-wheel scrolling in the window list"
msgstr "Activar el desplazamiento del ratón en la lista de ventanas"

#. 3.2->settings-schema.json->pref_window_preview_custom_height->description
#. 0dyseus@window-list-fork->settings-
#. schema.json->pref_window_preview_custom_height->description
msgid "Custom height for window thumbnails"
msgstr "Ancho personalizado para imágenes en miniatura de la ventana"

#. 3.2->settings-schema.json->pref_window_preview_custom_height->units
#. 3.2->settings-schema.json->pref_window_preview_custom_width->units
#. 0dyseus@window-list-fork->settings-
#. schema.json->pref_window_preview_custom_height->units
#. 0dyseus@window-list-fork->settings-
#. schema.json->pref_window_preview_custom_width->units
msgid "pixels"
msgstr "pixeles"

#. 3.2->settings-schema.json->pref_window_preview->description
#. 0dyseus@window-list-fork->settings-
#. schema.json->pref_window_preview->description
msgid "Show window thumbnails on hover"
msgstr "Mostrar imágenes en miniatura de la ventana al pasar el ratón"

#. 3.2->settings-schema.json->pref_pref_sub_menu_placement->description
#. 0dyseus@window-list-fork->settings-
#. schema.json->pref_pref_sub_menu_placement->description
msgid "Where to display the \"Preferences\" sub-menu?"
msgstr "¿Dónde mostrar el submenú \"Preferencias\"?"

#. 3.2->settings-schema.json->pref_pref_sub_menu_placement->tooltip
#. 0dyseus@window-list-fork->settings-
#. schema.json->pref_pref_sub_menu_placement->tooltip
msgid ""
"If the option \"Invert context menu items order\" is enabled, the \"Top of menu\" will actually "
"be the \"Bottom of menu\" and vice versa."
msgstr ""
"Si la opción \"Invertir el orden de los ítems en el menú contextual\" está activada, la parte "
"superior del menú será realmente la parte inferior y vice versa."

#. 3.2->settings-schema.json->pref_pref_sub_menu_placement->options
#. 0dyseus@window-list-fork->settings-
#. schema.json->pref_pref_sub_menu_placement->options
msgid "Do not display"
msgstr "No mostrar"

#. 3.2->settings-schema.json->pref_pref_sub_menu_placement->options
#. 0dyseus@window-list-fork->settings-
#. schema.json->pref_pref_sub_menu_placement->options
msgid "Top of menu"
msgstr "En la parte superior del menú"

#. 3.2->settings-schema.json->pref_pref_sub_menu_placement->options
#. 0dyseus@window-list-fork->settings-
#. schema.json->pref_pref_sub_menu_placement->options
msgid "Bottom of menu"
msgstr "En la parte inferior del menú"

#. 3.2->settings-schema.json->pref_window_preview_custom_width->description
#. 0dyseus@window-list-fork->settings-
#. schema.json->pref_window_preview_custom_width->description
msgid "Custom width for window thumbnails"
msgstr "Ancho personalizado para imágenes en miniatura de la ventana"

#. 3.2->settings-schema.json->pref_buttons_use_entire_space->description
#. 0dyseus@window-list-fork->settings-
#. schema.json->pref_buttons_use_entire_space->description
msgid "Window buttons can have different sizes and use the entire space available"
msgstr ""
"Los botones de las ventanas pueden tener diferentes tamaños y pueden usar todo el espacio "
"disponible"

#. 3.2->settings-schema.json->pref_middle_click_close->description
#. 0dyseus@window-list-fork->settings-
#. schema.json->pref_middle_click_close->description
msgid "Middle click to close window"
msgstr "Cerrar ventana con botón central del ratón"

#. 3.2->settings-schema.json->pref_hide_tooltips->description
#. 0dyseus@window-list-fork->settings-
#. schema.json->pref_hide_tooltips->description
msgid "Do not display tooltips"
msgstr "No mostrar caja de herramientas"

#. 3.2->settings-schema.json->pref_enable_alerts->description
#. 0dyseus@window-list-fork->settings-
#. schema.json->pref_enable_alerts->description
msgid "Show an alert in the window list when a window from another workspace requires attention"
msgstr ""
"Mostrar una alerta en la lista de ventanas cuando una ventana de otra área de trabajo requiere "
"atención"

#. 0dyseus@window-list-fork->metadata.json->description
msgid "Fork of the main Cinnamon Window list applet."
msgstr "Adaptación del applet de Cinnamon llamado Window list"

#. 0dyseus@window-list-fork->metadata.json->name
msgid "Window list (Fork By Odyseus)"
msgstr "Lista de ventanas (Fork By Odyseus)"

