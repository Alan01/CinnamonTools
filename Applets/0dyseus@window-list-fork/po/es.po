#: applet.js:775
msgid "Move to another workspace"
msgstr "Mover a otro entorno de trabajo"

#. Close all/others
#: applet.js:797
msgid "Close all"
msgstr "Cerrar todas"

#: applet.js:806
msgid "Close others"
msgstr "Cerrar otras"

#: applet.js:820
msgid "Restore to full opacity"
msgstr "Dummy"

#: applet.js:828
msgid "Restore"
msgstr "Restaurar"

#: applet.js:833
msgid "Minimize"
msgstr "Minimizar"

#: applet.js:841
msgid "Unmaximize"
msgstr "Desmaximizar"

#: applet.js:846
